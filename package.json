{
  "name": "cat",
  "productName": "cat",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "husky install && run-s pysetup",
    "pysetup": "run-script-os",
    "pysetup:win32": "powershell py/pysetup.ps1 || cmd pysetup.bat",
    "pysetup:darwin:linux": "bash py/pysetup.sh",
    "pysetup:default": "bash py/pysetup.sh"
  },
  "runtimeArgs": [
    "--trace-warnings"
  ],
  "keywords": [
    "cnc",
    "pcb",
    "leveling",
    "contouring",
    "angular",
    "react"
  ],
  "author": {
    "name": "Blake Lucas",
    "email": "blakeglucas@gmail.com"
  },
  "license": "MIT",
  "overrides": {
    "@reach/dialog": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-simple-snackbar": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@types/colornames": "^1.1.2",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-resizable": "^3.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.0.0",
    "electron": "22.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "node-loader": "^2.0.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "run-script-os": "^1.1.6",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@iconscout/react-unicons": "^1.1.6",
    "@popperjs/core": "^2.11.6",
    "@reach/dialog": "^0.18.0",
    "@react-three/drei": "^9.46.4",
    "@react-three/fiber": "^8.9.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@tippyjs/react": "^4.2.6",
    "@types/three": "^0.146.0",
    "clsx": "^1.2.1",
    "colornames": "^1.1.1",
    "custom-electron-titlebar": "^4.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "path-browserify": "^1.0.1",
    "rc-slider": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown-select": "^4.9.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.5",
    "react-rnd": "^10.3.7",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.4.5",
    "react-simple-snackbar": "^1.1.11",
    "react-spring": "^9.5.5",
    "redux-persist": "^6.0.0",
    "serialport": "^10.5.0",
    "tailwind-merge": "^1.8.0",
    "three": "^0.147.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
